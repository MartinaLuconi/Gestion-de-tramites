<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.Documentacion" table="documentacion">

        <id name="OID" type="string">
            <column name="OIDDocumentacion"/>
            <generator class="uuid2"/>
        </id>

        <property name="codDocumentacion" type="int">
            <column name="codDocumentacion"/>
        </property>

        <property name="nombreDocumentacion" type="string">
            <column name="nombreDocumentacion"/>
        </property>

        <property name="fechaHoraBajaDocumentacion" column="fechaHoraBajaDocumentacion" type="timestamp"/>
        
        
    </class>
    
</hibernate-mapping>
