<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.TramiteEstado" table="tramiteEstado">

        <id name="OID" type="string">
            <column name="OIDTramiteEstado"/>
            <generator class="uuid2"/>
        </id>

        <property name="nroLineaTramiteEstado" type="int">
            <column name="nroLineaTramiteEstado"/>
        </property>

        <property name="observacionTramiteEstado" type="string">
            <column name="observacionTramiteEstado"/>
        </property>
        
        <property name="fechaHoraDesdeTramiteEstado" column="fechaHoraDesdeTramiteEstado" type="timestamp" not-null="true"/>

        <property name="fechaHoraHastaTramiteEstado" column="fechaHoraHastaTramiteEstado" type="timestamp"/>

        <property name="fechaHoraBajaTramiteEstado" column="fechaHoraBajaTramiteEstado" type="timestamp"/>
        
        <many-to-one name="estadoTramite" class="entidades.EstadoTramite" column="OIDEstadoTramite" not-null="true"/>
    
    </class>
    
</hibernate-mapping>
