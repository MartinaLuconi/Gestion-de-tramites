<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.DetallePrecioTipoTramite" table="detallePrecioTipoTramite">

        <id name="OID" type="string">
            <column name="OIDDetallePrecioTipoTramite"/>
            <generator class="uuid2"/>
        </id>

        <property name="precioTipoTramite" type="double">
            <column name="precioTipoTramite"/>
        </property>
        
        <many-to-one name="tipoTramite" class="entidades.TipoTramite" column="OIDTipoTramite" not-null="true"/>
        
        <many-to-one name="listaPrecio" class="entidades.ListaPrecio" column="OIDListaPrecio" not-null="true"/>
        
    </class>

    
</hibernate-mapping>
