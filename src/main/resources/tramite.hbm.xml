<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.Tramite" table="tramite">
        
        <id name="OID" type="string">
            <column name="OIDTramite"/>
            <generator class="uuid2"/>
        </id>

        <property name="nroTramite" type="int">
            <column name="nroTramite"/>
        </property>

        <property name="precioTramite" type="float">
            <column name="precioTramite"/>
        </property>
        

        <property name="fechaHoraCargaTramite" column="fechaHoraCargaTramite" type="timestamp" not-null="true"/>
        
        <property name="fechaHoraVencimientoDocumentacion" column="fechaHoraVencimientoDocumentacion" type="timestamp" not-null="true"/>
       
        <property name="fechaHoraFinTramite" column="fechaHoraFinTramite" type="timestamp"/>
        
        <property name="fechaHoraFinEntregaDocumentacion" column="fechaHoraFinEntregaDocumentacion" type="timestamp"/>
        
        <property name="fechaHoraBajaTramite" column="fechaHoraBajaTramite" type="timestamp"/>

        <many-to-one name="consultor" class="entidades.Consultor" column="OIDConsultor" not-null="true"/>
        
        <many-to-one name="cliente" class="entidades.Cliente" column="OIDCliente" not-null="true"/>
       
        <many-to-one name="tipoTramite" class="entidades.TipoTramite" column="OIDTipoTramite" not-null="true"/>
        
        <many-to-one name="version" class="entidades.Version" column="OIDVersion" not-null="true"/>
        
        <many-to-one name="estadoTramite" class="entidades.EstadoTramite" column="OIDEstadoTramite" not-null="true"/>
        
        <bag name="tramiteDocumentacionList" table="tramiteDocumentacion" cascade="save-update" inverse="false">
            <key column="OIDTramite" not-null="false"/>
            <one-to-many class="entidades.TramiteDocumentacion"/>
        </bag>
        
        <bag name="tramiteEstadoList" table="tramiteEstado" cascade="save-update" inverse="false">
            <key column="OIDTramite"/>
            <one-to-many class="entidades.TramiteEstado"/>
        </bag>
        
    </class>

</hibernate-mapping>

